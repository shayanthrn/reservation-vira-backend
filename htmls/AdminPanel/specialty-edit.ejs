  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">
  <title>اضافه کردن دسته بندی - دکتر تجویز</title>
  <link rel="icon" href="favicon.ico">

  <title></title>

  <link rel="stylesheet" href="https://cdn.rtlcss.com/bootstrap/v4.1.3/css/bootstrap.min.css" integrity="sha384-Jt6Tol1A2P9JBesGeCxNrxkmRFSjWCBW1Af7CSQSKsfMVQCqnUVWhZzG0puJMCK6" crossorigin="anonymous">
  <link href="assets/panelassests/dashboard.rtl.css" rel="stylesheet">
  <script src="https://use.fontawesome.com/6d68f18b97.js"></script>
  <link href="https://use.fontawesome.com/6d68f18b97.css" media="all" rel="stylesheet">

  <link rel="stylesheet" href="https://unpkg.com/persian-datepicker@latest/dist/css/persian-datepicker.min.css">
  <link href="assets/css/main.css" rel="stylesheet" />
</head>

<body>

  <nav class="navbar fixed-top navbar-expand-lg nav-dark">
    <a class="navbar-brand mr-2" style="color: white;"> دکتر تجویز</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo" aria-controls="navbarTogglerDemo" aria-expanded="false" aria-label="Toggle navigation">
      <i class="fa fa-bars menu-icon"></i>
    </button>
  </nav>

  <div class="container-fluid">
    <div class="row">
      
      <nav class="col-lg-3 col-xl-2 d-lg-block sidebar collapse" id="navbarTogglerDemo">
        <div class="sidebar-sticky">
          <ul class="nav flex-column">
            
            <li class="nav-item">
              <a class="nav-link " href="/adminpanel/dashboard" >
                <i class="fa fa-tachometer"></i>
                داشبورد
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link " href="/adminpanel/chats" >
                <i class="fa fa-commenting"></i>
                گفتگو های متنی
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/AdminPanel/telereserves" >
                <i class="fa fa-phone"></i>
                رزرو های تلفنی
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/adminpanel/doctors" >
                <i class="fa fa-user-md"></i>
                دکتر ها
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/adminpanel/healthcenters" >
                <i class="fa fa-heartbeat"></i>
                مراکز درمانی
              </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/adminpanel/visittimes">
                  <i class="fa fa-calendar"></i>
                  تعیین زمانبندی
                </a>
            </li>
            <li class="nav-item">
              <a class="nav-link " href="/adminpanel/costmanage">
                <i class="fa fa-calendar"></i>
                تعیین هزینه ها
              </a>
          </li>
            <li class="nav-item">
              <a class="nav-link active" href="/adminpanel/categories" >
                <i class="fa fa-book"></i>
                تخصص ها
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/adminpanel/patients" >
                <i class="fa fa-users"></i>
                بیماران
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/adminpanel/transactions" >
                <i class="fa fa-money"></i>
                تراکنش های مالی
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/exit" >
                <i class="fa fa-sign-out"></i>
                خروج
              </a>
            </li>
          </ul>

          </div>
      </nav>


      <main role="main" class=" ml-sm-auto col-lg-9 col-xl-10 px-4" id="specialty-edit">
        <div class="container-fluid">
          <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
            <h1 class="h2">اصلاح کردن تخصص</h1>
          </div>

          <form action="editcategory" enctype="multipart/form-data" method="POST">
            <div class="row">
              <div class="col-sm-6 col-md-2 mb-5">
                <img class="img-fluid" src="<%= category.image %>" onerror="this.src='assets/img/altimage.png'">
                <div class="form-group mt-2">
                  <input type="file" class="form-control-file" name="image" id="image">
                </div>
              </div>
              <div class="col-sm-12 col-md-10">
                <div class="row">
                  <div class="col-md-4 col-lg-2">
                    <h5>عنوان تخصص</h5>
                  </div>
                  <div class="col-md-6 col-lg-6">
                    <div class="form-group mb-2">
                      <input type="text" class="form-control form-control-sm" name="name" id="name" placeholder="تخصص" value="<%= category.name %>" disabled>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <hr>
            <div class="row">
              <div class="col">
                <div class="float-right">
                  <button type="submit" class="btn btn-success edit-button">
                    <i class="fa fa-edit"></i>
                    اصلاح
                  </button>
                </div>
              </div>
            </div>
          </form>
        </div>
      </main>
      
    </div>
  </div>
  


  <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
  <script>window.jQuery || document.write('<script src="assets/panelassests/jquery-slim.min.js"><\/script>')</script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
  <script src="https://cdn.rtlcss.com/bootstrap/v4.1.3/js/bootstrap.min.js" integrity="sha384-C/pvytx0t5v9BEbkMlBAGSPnI1TQU1IrTJ6DJbC8GBHqdMnChcb6U4xg4uRkIQCV" crossorigin="anonymous"></script>

</body>
</html>
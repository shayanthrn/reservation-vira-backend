      <main role="main" class=" ml-sm-auto col-lg-9 col-xl-10 px-4" id="addClosedTime">
        <div class="container-fluid">

          <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
            <h1 class="h2">تعیین هزینه</h1>
          </div>
          
          <form class="" autocomplete="off" action="changecostadmin" method="GET">
            <div class="row">
              <div class="col-xl-4 col-lg-6">
                <div class="" action="" method="GET">
                  <div class="row">
                    <div class="col">
                        <div class="form-group">
                          <label for="type">انتخاب دسته بندی </label>
                          <select class="form-control" onchange="myfunc()" name="type" id="type" required>
                            <option value="doctor">دکتر</option>
                            <option value="آزمایشگاه">آزمایشگاه</option>
                            <option value="کلینیک">کلینیک</option>
                          </select>
                          <select class="form-control" name="visittype" id="visittype">
                            <option value="reserve">رزرو
                            </option>
                            <option value="chat">گفتگو متنی
                            </option>
                            <option value="tele">گفتگو تلفنی
                            </option>
                          </select>
                        </div>
                    </div>
                    <div class="col">
                      <div class="form-group">
                        <label for="cost">
                          هزینه ( تومان)
                        </label>
                        <input type="text" class="form-control" name="cost" id="cost" required/>
                      </div>
                    </div>
                  </div>
                </div>
                <button type="submit" class="btn btn-success submit-button float-right">
                  <i class="fa fa-check"></i>
                  ثبت
                </button>
              </div>
            </div>
          </form>

      </main>
    </div>
  </div>

  <script>
    function myfunc(){
      if(document.getElementById("type").value=="doctor"){
        document.getElementById("visittype").style.display="";
      }
      else{
        document.getElementById("visittype").style.display="none";
      }
    }
  </script>

